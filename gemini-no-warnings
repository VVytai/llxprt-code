#!/usr/bin/env node

// Suppress specific deprecation warnings before they occur
process.on('warning', (warning) => {
  if (warning.name === 'DeprecationWarning' && 
      (warning.message.includes('punycode') || 
       warning.message.includes('url.parse') ||
       warning.code === 'DEP0040' ||
       warning.code === 'DEP0169')) {
    return; // Ignore these specific warnings
  }
  console.warn(warning); // Show other warnings
});

// Now run the actual gemini CLI
import('./packages/cli/dist/index.js');